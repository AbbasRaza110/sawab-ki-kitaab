{
  "expo": {
    "name": "sawab-ki-kitaab",
    "slug": "sawab-ki-kitaab",
    "version": "1.0.0",
    "orientation": "portrait",

    "icon": "./assets/images/icon1.png",

    "scheme": "sawabkikitaab",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/icon1.png"
    },

    "android": {
      "icon": "./assets/images/icon1.png",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.sawabkikitaab"
    },

    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon2.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-router"
    ],

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
